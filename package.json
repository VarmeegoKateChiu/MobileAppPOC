{
  "name": "reactMobileAppDemo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "android-dev": "npx react-native run-android --mode=devDebug",
    "android-prod": "npx react-native run-android --mode=release",
    "ios-dev": "npx react-native run-ios --configuration=Dev",
    "ios-prod": "npx react-native run-ios --configuration=Release",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "win-aos:dev": "npx react-native run-android --variant=developmentDebug --appIdSuffix development"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.15",
    "@mui/material": "^5.14.15",
    "@react-native-async-storage/async-storage": "^1.19.5",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-cookies/cookies": "^6.2.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "expo": "^49.0.18",
    "expo-secure-store": "~12.3.1",
    "react": "18.2.0",
    "react-native": "0.72.5",
    "react-native-config": "^1.5.1",
    "react-native-elements": "^3.4.2",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-paper": "^5.11.1",
    "react-native-reanimated": "^3.5.4",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^3.4.1",
    "react-native-screens": "^3.27.0",
    "react-native-vector-icons": "^10.0.1",
    "react-native-webview": "^13.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@tsconfig/react-native": "^3.0.0",
    "@types/react": "^18.2.33",
    "@types/react-native": "^0.72.5",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-native-typescript-transformer": "^1.2.13",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
